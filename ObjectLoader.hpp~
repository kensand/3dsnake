#ifndef OBJLOADER_H
#define OBJLOADER_H
#include <vector>

bool loadOBJ(
	const char * path, 
	std::vector<GLfloat[3]> & out_vertices, 
	std::vector<GLfloat[2]> & out_uvs, 
	std::vector<GLfloat[3]> & out_normals
);



bool loadAssImp(
	const char * path, 
	std::vector<unsigned short> & indices,
	std::vector<GLfloat[3]> & vertices,
	std::vector<GLfloat[2]> & uvs,
	std::vector<GLfloat[3]> & normals
);

#endif
